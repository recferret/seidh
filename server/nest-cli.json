    "@app/collection": {
      "type": "application",
      "root": "apps/collection",
      "entryFile": "main",
      "sourceRoot": "apps/collection/src",
      "compilerOptions": {
        "tsConfigPath": "apps/collection/tsconfig.app.json"
      }
    },
    "@app/gameplay-worker": {
      "type": "application",
      "root": "apps/gameplay-worker",
      "entryFile": "main",
      "sourceRoot": "apps/gameplay-worker/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gameplay-worker/tsconfig.app.json"
      }
    },
    "@app/game": {
      "type": "application",
