{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "monorepo": true,
  "root": "apps",
  "projects": {
    "@app/ws-gateway": {
      "type": "application",
      "root": "apps/ws-gateway",
      "sourceRoot": "apps/ws-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ws-gateway/tsconfig.app.json"
      }
    },
    "@app/gameplay": {
      "type": "application",
      "root": "apps/gameplay",
      "sourceRoot": "apps/gameplay/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gameplay/tsconfig.app.json"
      }
    },
    "@app/gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "@app/gameplay-lobby": {
      "type": "application",
      "root": "apps/gameplay-lobby",
      "entryFile": "main",
      "sourceRoot": "apps/gameplay-lobby/src",
