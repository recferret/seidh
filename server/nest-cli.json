      }
    },
    "@app/users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    },
    "@app/boosts": {
      "type": "application",
      "root": "apps/boosts",
      "entryFile": "main",
      "sourceRoot": "apps/boosts/src",
      "compilerOptions": {
        "tsConfigPath": "apps/boosts/tsconfig.app.json"
      }
    },
    "@app/characters": {
      "type": "application",
      "root": "apps/characters",
      "entryFile": "main",
      "sourceRoot": "apps/characters/src",
      "compilerOptions": {
